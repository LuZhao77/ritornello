{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
            "error",
            { "argsIgnorePattern": "next|options|pw" }
        ],
        "no-console": "off"
    },
    "globals": {
        "CONFIG": true,
        "to": true,
        "pe": true,
        "TE": true,
        "ReE": true,
        "ReS": true
    }
}